<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attention.newsviews">

    <application
        android:allowBackup="true"
        android:icon="@drawable/newsview"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".NewsPaperActivity" />
        <activity android:name=".NewsWebView_Activity" />
        <activity android:name=".MegazineActivity" />
        <activity android:name=".megazineWeb_view"></activity>
    </application>

</manifest>